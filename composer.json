{
    "name": "eonx-com/easy-standard",
    "description": "Centralised source of coding standard classes",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.4",
        "slevomat/coding-standard": "^6.1",
        "symplify/easy-coding-standard-tester": "^7.2",
        "rector/rector": "0.7.7"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyStandard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyStandard\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
